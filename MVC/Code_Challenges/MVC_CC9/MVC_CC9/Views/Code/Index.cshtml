@model IEnumerable<MVC_CC9.Models.Customer>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Cutsomer in Germany", "Germany") | @Html.ActionLink("Search By OrderID", "Order")
</p>

<table class="table table-bordered"">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CustomerID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CompanyName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContactName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.City)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Country)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Phone)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <th>
                @Html.DisplayFor(models => item.CustomerID)
            </th>
            <th>
                @Html.DisplayFor(models => item.CompanyName)
            </th>
            <th>
                @Html.DisplayFor(models => item.ContactName)
            </th>
            <th>
                @Html.DisplayFor(models => item.City)
            </th>
            <th>
                @Html.DisplayFor(models => item.Country)
            </th>
            <th>
                @Html.DisplayFor(models => item.Phone)
            </th>
        </tr>
    }
</table>



